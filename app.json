{
  "expo": {
    "name": "expo-user-management",
    "slug": "expo-user-management",
    "scheme": "expousermangment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/terre.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/terre.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/terre.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.expousermanagement"
    },
    "web": {
      "favicon": "./assets/terre.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router"     
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "511b4eee-5d65-450f-9e97-01820fc9076a"
      }
    },
    "owner": "myohann"
  }
}
