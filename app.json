{
  "expo": {
    "name": "expo-user-management",
    "slug": "expo-user-management",
    "scheme": "expousermangment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/terre.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/terre.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/terre.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.expousermanagement"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router"
    ]
  }
}
